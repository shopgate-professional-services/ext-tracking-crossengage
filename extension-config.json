{
  "version": "1.1.0",
  "id": "@shopgate-project/tracking-crossengage",
  "components": [
    {
      "id": "init",
      "path": "frontend/index.js",
      "type": "tracking"
    }
  ],
  "configuration": {
    "xngID": {
      "type": "admin",
      "destination": "frontend",
      "default": "",
      "params": {
        "label": "xngID",
        "type": "text"
      }
    },
    "additionalOptions": {
      "type": "admin",
      "destination": "frontend",
      "default": {
        "businessUnit": "App DE"
      },
      "params": {
        "label": "additionalOptions for SDK",
        "type": "json"
      }
    },
    "trackOrders": {
      "type": "admin",
      "destination": "frontend",
      "default": true,
      "params": {
        "label": "Flag if orders should be tracked or not",
        "type": "checkbox"
      }
    }
  }
}
